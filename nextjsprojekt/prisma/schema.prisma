// prisma/schema.prisma
generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/client"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

// prisma/schema.prisma
model InventoryItem {
  id       Int      @id @default(autoincrement())
  itemName String
  quantity Int
}
